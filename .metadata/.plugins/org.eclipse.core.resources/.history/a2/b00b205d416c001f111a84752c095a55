package com.org.jdbcdriver;

import java.sql.*;

public class StatementInterfaceUse {

	public static final String url="jdbc:mysql://localhost:3306/mydb";
	public static final String userName="root";
	public static final String password="Ganesh4237@";

	public static void main(String[] args) throws SQLException {
		com.mysql.cj.jdbc.Driver dr=new com.mysql.cj.jdbc.Driver();
		DriverManager.registerDriver(dr);
		Connection con=DriverManager.getConnection("url","userName","password");
		if(con!=null)
		{
			System.out.println("correct");
			Statement sta=con.createStatement();
			int val=sta.executeUpdate("insert into register value('New','newconnection.gmail.com','86374575')");
			if(val>0)
			{
				System.out.println("Comand executed");
			}
			else
			{
				System.out.println("Update not executed");
			}
		}
		else
		{
			System.out.println("Database Not Connected");
		}
		
	}

}
